<div class="page-title">
    <div class="title">{{i18n.editNomenclature}}</div>
    <a href="/nomenclature" class="link-back">{{i18n.nomenclature}}</a>
</div>
<form data-submit="update">
    {{> app/company/views/nomenclature/form.hbs }}
    <div class="page-section">
        <div class="col col-stretch form-control-static">
            <button type="submit" class="btn btn-primary" data-auto-disable>{{i18n.save}}</button>
            <a href="/nomenclature/{{id}}" class="btn btn-link">{{i18n.cancel}}</a>
        </div>
    </div>
</form>